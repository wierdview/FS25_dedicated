# Copy this file to .env and customize the values

# VNC Settings
# Password for VNC access (minimum 6 characters)
VNC_PASSWORD=changeme

# FS25 Web Admin Settings (Port 8080)
# These credentials are used to access the FS25 web administration panel
WEB_USERNAME=admin
WEB_PASSWORD=changeme

# User/Group IDs
# Set these to match your host user to avoid permission issues
PUID=1000
PGID=1000

# Server Configuration
SERVER_NAME=My FS25 Server
SERVER_PASSWORD=
SERVER_ADMIN=adminpassword
SERVER_PLAYERS=16
SERVER_PORT=10823
SERVER_REGION=en
SERVER_MAP=MapUS
SERVER_DIFFICULTY=3
SERVER_PAUSE_EMPTY=2
SERVER_SAVE_INTERVAL=180.000000
SERVER_STATS_INTERVAL=31536000
SERVER_CROSSPLAY=true
ENABLE_STARTUP_SCRIPTS=yes